<div class="col-md-12">
  <div class="group">
    <%= link_to("创建新团队", new_team_path, class: "btn btn-primary pull-right") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>团队名字</td>
        <td>团队介绍</td>
        <td>领队</td>
      </tr>
    </thead>
    <tbody>
      <%= render :partial => "team_item", :collection => @teams, :as => :team %>
    </tbody>
  </table>
</div>
