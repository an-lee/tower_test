<div class="col-md-12">
  <div class="group">
    <%= link_to("返回团队", team_path(@team), class:"btn btn-default pull-right") %>
    <%= link_to("新建项目", new_team_project_path, class: "btn btn-primary pull-right") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>项目名称</td>
        <td>项目描述</td>
        <td>项目创建人</td>
      </tr>
    </thead>
    <tbody>
      <%= render :partial => "project_item", :collection => @projects, :as => :project %>
    </tbody>
  </table>
</div>
