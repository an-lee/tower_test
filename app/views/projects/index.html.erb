<div class="col-md-12">
  <div class="group">
    <%= link_to("New Project", new_project_path, class: "btn btn-primary pull-right") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>Title</td>
        <td>Description</td>
        <td>Creator</td>
      </tr>
    </thead>
    <tbody>
      <%= render :partial => "project_item", :collection => @projects, :as => :project %>
    </tbody>
  </table>
</div>
